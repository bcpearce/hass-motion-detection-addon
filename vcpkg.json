{
  "dependencies": [
    "boost-program-options",
    "boost-url",
    "curl",
    "live555",
    "ms-gsl",
    "mongoose",
    {
      "name": "opencv4",
      "default-features": false,
      "features": [
        "contrib",
        "jpeg",
        "png",
        "tiff"
      ]
    },
    "openh264",
    "openssl",
    "nlohmann-json"
  ],
  "features": {
    "graphical-user-interface": {
      "dependencies": [
        {
          "name": "opencv4",
          "features": [
            "highgui"
          ]
        }
      ],
      "description": "Show a graphical user interface while running."
    },
    "tests": {
      "dependencies": [
        "gtest"
      ],
      "description": "Build tests."
    }
  }
}
